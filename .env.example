# BasketStat Environment Configuration
# Copy this file to .env and fill in your values

# JSONbin.io Configuration
# Get your API key at: https://jsonbin.io/app/api-keys
JSONBIN_API_KEY=your_api_key_here
JSONBIN_BIN_ID=your_bin_id_here

# Application Password (required - used for login)
APP_PASSWORD=your_password_here

# Session Secret (optional - defaults to APP_PASSWORD if not set)
# Used to sign session cookies. Use a long random string in production.
# SESSION_SECRET=your_random_secret_here

# Server Configuration
PORT=3000
